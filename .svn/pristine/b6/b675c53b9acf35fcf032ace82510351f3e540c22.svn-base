package com.ikkong.dg.model.json;

import java.io.Serializable;

@SuppressWarnings("serial")
public class AcceptInfo implements Serializable {
	//用户昵称,手机号,用户头像,点评人数,评分,工种
	private Long uid;
	private String name;
	private String phoneno;
	private String headIco;
	private Integer customs;//点评总人数
	private Integer total;//点评总分数，5星评分
	private String worktypes;//所带工种
	
	public Long getUid() {
		return uid;
	}
	public void setUid(Long uid) {
		this.uid = uid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPhoneno() {
		return phoneno;
	}
	public void setPhoneno(String phoneno) {
		this.phoneno = phoneno;
	}
	public String getHeadIco() {
		return headIco;
	}
	public void setHeadIco(String headIco) {
		this.headIco = headIco;
	}
	public Integer getCustoms() {
		return customs;
	}
	public void setCustoms(Integer customs) {
		this.customs = customs;
	}
	public Integer getTotal() {
		return total;
	}
	public void setTotal(Integer total) {
		this.total = total;
	}
	public String getWorktypes() {
		return worktypes;
	}
	public void setWorktypes(String worktypes) {
		this.worktypes = worktypes;
	}
}
