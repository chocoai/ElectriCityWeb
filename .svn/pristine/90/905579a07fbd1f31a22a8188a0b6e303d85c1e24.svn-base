@layout("/common/_curd/_container.html"){

	<form class="form-horizontal" role="form">
		<!-- 表单 -->
		@ var _col=[
		@			  {name:"文件名", index:"file_name", type:"text",newline:true,length:8},
		@			  {name:"文件后缀", index:"file_postfix", type:"text",newline:true,length:8},
		@			  {name:"文件路径", index:"file_path", type:"text",newline:true,length:8},
		@			  {name:"所属对象id", index:"object_id", type:"text",newline:true,length:8},
		@			  {name:"文件所属类别", index:"object_type", type:"text",newline:true,length:8},
		@			  {name:"创建时间", index:"create_tm", type:"text",newline:true,length:8}	
		@		   ];
		@ include("/common/_curd/_view.html", {"col":_col}){}
		
		<!-- 按钮 -->
		@ include("/common/_curd/_btn.html", {btn_close:"关闭"}) {}
	</form>

@}
