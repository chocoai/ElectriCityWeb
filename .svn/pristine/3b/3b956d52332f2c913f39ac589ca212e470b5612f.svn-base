package com.ikkong.dg.model.json;

import java.io.Serializable;

/**
 * 项目结算信息
 * 
 * @author skybiran
 *
 */
@SuppressWarnings("serial")
public class ItemBill implements Serializable {

	private Long id;//项目ID
	private Double subsidyMoney;//补贴金额
	private Double overMoney;//加班酬劳
	private Integer	overDays;//加班天数
	private Integer factDays;//工程实际天数
	private Double 	total;//总金额
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public Double getOverMoney() {
		return overMoney;
	}
	public void setOverMoney(Double overMoney) {
		this.overMoney = overMoney;
	}
	public Integer getOverDays() {
		return overDays;
	}
	public void setOverDays(Integer overDays) {
		this.overDays = overDays;
	}
	public Integer getFactDays() {
		return factDays;
	}
	public void setFactDays(Integer factDays) {
		this.factDays = factDays;
	}
	public Double getTotal() {
		return total;
	}
	public void setTotal(Double total) {
		this.total = total;
	}
	public Double getSubsidyMoney() {
		return subsidyMoney;
	}
	public void setSubsidyMoney(Double subsidyMoney) {
		this.subsidyMoney = subsidyMoney;
	}
	
	
}
