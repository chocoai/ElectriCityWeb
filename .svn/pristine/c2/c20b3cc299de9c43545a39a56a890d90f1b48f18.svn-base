@layout("/common/_curd/_container.html"){

	<form class="form-horizontal" role="form">
		<!-- 表单 -->
		@ var _col=[
			
@			  {name:"", index:"id", type:"text",newline:true,length:8},
		@			  {name:"电工用户id", index:"create_id", type:"text",newline:true,length:8},
		@			  {name:"订单id", index:"order_id", type:"text",newline:true,length:8},
		@			  {name:"订单周期", index:"order_date", type:"text",newline:true,length:8},
		@			  {name:"工种类型id", index:"worktype_id", type:"text",newline:true,length:8},
		@			  {name:"项目描述", index:"mark", type:"text",newline:true,length:8},
		@			  {name:"项目金额", index:"money", type:"text",newline:true,length:8},
		@			  {name:"项目状态", index:"status", type:"text",newline:true,length:8},
		@			  {name:"接单人id", index:"accpet_user", type:"text",newline:true,length:8},
