		@			  {name:"补贴说明", index:"subsidy_mark", type:"text",newline:true,length:8},
		@			  {name:"版本记录号", index:"version", type:"text",newline:true,length:8}		
		@		   ];
		@ include("/common/_curd/_view.html", {"col":_col}){}
		
